<?xml version="1.0"?>
<block>
  <name>addgsmtr26_bb</name>
  <key>viterbi_addgsmtr26_bb</key>
  <category>viterbi</category>
  <import>import viterbi</import>
  <make>viterbi.addgsmtr26_bb()</make>

  <sink>
    <name>in</name>
    <type>byte</type>
  </sink>

  <!-- Make one 'source' node per output. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <source>
    <name>out</name>
     <type>byte</type>
  </source>
</block>
