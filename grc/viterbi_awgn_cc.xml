<?xml version="1.0"?>
<block>
  <name>awgn_cc</name>
  <key>viterbi_awgn_cc</key>
  <category>viterbi</category>
  <import>import viterbi</import>
  <make>viterbi.awgn_cc($std, $seed)</make>
  

<param>
    <name>Std deviation</name>
    <key>std</key>
    <value>1</value>
    <type>real</type>
  </param>

  <param>
    <name>Seed</name>
    <key>seed</key>
    <value>0</value>
    <type>int</type>
  </param>

  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>

  <source>
    <name>out</name>
    <type>complex</type>
  </source>
</block>
